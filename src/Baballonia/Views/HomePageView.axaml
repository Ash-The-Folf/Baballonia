<UserControl
    x:Class="Baballonia.Views.HomePageView"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:splitViewPane="clr-namespace:Baballonia.ViewModels.SplitViewPane"
    xmlns:views="clr-namespace:Baballonia.Views"
    d:DesignHeight="450"
    d:DesignWidth="800"
    x:DataType="splitViewPane:HomePageViewModel"
    mc:Ignorable="d">

    <ScrollViewer>
        <StackPanel Margin="16" Classes="Page">

            <Grid Margin="0,0,0,12" ColumnDefinitions="*,Auto">
                <TextBlock
                    Grid.Column="0"
                    Classes="Header"
                    FontSize="28"
                    Text="Home" />
                <!-- TODO: Handle -->
                <SplitButton
                    x:Name="SplitCalibrationButton"
                    Grid.Column="1"
                    Padding="12,8"
                    CornerRadius="4"
                    IsEnabled="{Binding ShouldShowEyeCalibration}"
                    IsVisible="{Binding ShouldShowEyeCalibration}">
                    <SplitButton.Content>
                        <StackPanel Orientation="Horizontal" Spacing="8">
                            <PathIcon Data="{StaticResource HeadsetVrRegular}" />
                            <TextBlock VerticalAlignment="Center" Text="{Binding SelectedCalibrationText}" />
                        </StackPanel>
                    </SplitButton.Content>
                    <SplitButton.Flyout>
                        <MenuFlyout>
                          <!-- TODO: Handle -->
                            <MenuItem Header="5-Point Quick Calibration">
                                <MenuItem.Icon>
                                    <PathIcon Data="{StaticResource HeadsetVrRegular}" />
                                </MenuItem.Icon>
                            </MenuItem>
                            <MenuItem Header="Full Calibration">
                              <!-- TODO: Handle -->
                                <MenuItem.Icon>
                                    <PathIcon Data="{StaticResource HeadsetVrRegular}" />
                                </MenuItem.Icon>
                            </MenuItem>
                        </MenuFlyout>
                    </SplitButton.Flyout>
                </SplitButton>
            </Grid>

            <UniformGrid
                x:Name="UniformGridPanel"
                ColumnSpacing="12"
                Columns="1"
                RowSpacing="12"
                Rows="3">
                <!--  Left Panel  -->
                <Grid
                    IsEnabled="{Binding ShouldShowEyeCalibration}"
                    IsVisible="{Binding ShouldShowEyeCalibration}"
                    VerticalAlignment="Top"
                    RowDefinitions="Auto,Auto,Auto,Auto">
                    <TextBlock
                        Grid.Row="0"
                        Margin="0,0,12,12"
                        FontSize="20"
                        FontWeight="Medium"
                        Text="Left Eye Camera" />

                    <!--  Camera address section  -->
                    <Border
                        Grid.Row="1"
                        Margin="0,0,0,16"
                        Padding="16,16,16,16"
                        Background="{DynamicResource SystemChromeMediumLowColor}"
                        CornerRadius="8">
                        <StackPanel>
                            <TextBlock FontSize="14" Text="Left Camera Address" />

                            <Grid RowDefinitions="Auto,Auto,Auto">
                                <AutoCompleteBox
                                    x:Name="LeftCameraAddressEntry"
                                    Grid.Row="0"
                                    Height="36"
                                    Margin="0,6"
                                    FilterMode="Contains"
                                    FontSize="13"
                                    Text="{Binding LeftCamera.DisplayAddress}"
                                    ItemsSource="{Binding LeftCamera.Suggestions}"
                                    Watermark="Start typing..." />

                                <TextBlock
                                    x:Name="LeftAddressHint"
                                    Grid.Row="1"
                                    Margin="0,0,0,4"
                                    FontSize="12"
                                    FontStyle="Italic"
                                    Foreground="Orange"
                                    Text="{Binding LeftCamera.Hint}"
                                    IsVisible="False"
                                    TextWrapping="Wrap" />

                                <Grid
                                    Grid.Row="2"
                                    Margin="0,4,0,8"
                                    ColumnDefinitions="*,*">

                                    <Button
                                        Grid.Column="0"
                                        Margin="0,0,4,0"
                                        Padding="12,8"
                                        HorizontalAlignment="Stretch"
                                        HorizontalContentAlignment="Center"
                                        Command="{Binding CameraStartCommand}"
                                        CommandParameter="{Binding LeftCamera}"
                                        Content="Start Camera"
                                        FontSize="13" />

                                    <Button
                                        Grid.Column="1"
                                        Margin="4,0,0,0"
                                        Padding="12,8"
                                        HorizontalAlignment="Stretch"
                                        HorizontalContentAlignment="Center"
                                        Command="{Binding CameraStopCommand}"
                                        CommandParameter="{Binding LeftCamera}"
                                        Content="Stop Camera"
                                        FontSize="13" />
                                </Grid>
                            </Grid>

                            <StackPanel Orientation="Horizontal" Spacing="8">
                                <RadioButton
                                    x:Name="LeftTrackingModeButton"
                                    Content="Tracking"
                                    GroupName="LeftImageControlGroup"
                                    IsChecked="{Binding !LeftCamera.IsCropMode, Mode=TwoWay}" />
                                <RadioButton
                                    x:Name="LeftCroppingModeButton"
                                    Content="Cropping"
                                    IsChecked="{Binding LeftCamera.IsCropMode, Mode=TwoWay}"
                                    GroupName="LeftImageControlGroup" />
                            </StackPanel>

                            <Button
                                x:Name="LeftSelectEntireFrame"
                                IsVisible="{Binding LeftCamera.IsCropMode}"
                                Margin="0,8,0,0"
                                Padding="0,8"
                                HorizontalAlignment="Stretch"
                                HorizontalContentAlignment="Center"
                                Command="{Binding SelectWholeFrameCommand}"
                                CommandParameter="{Binding LeftCamera}"
                                Content="Select Left Frame"
                                FontSize="13" />
                        </StackPanel>
                    </Border>

                    <!--  Camera view section  -->
                    <Viewbox
                        IsVisible="{Binding LeftCamera.IsCameraStarted}"
                        x:Name="LeftViewBox"
                        Grid.Row="2"
                        Margin="0 0 0 16">
                        <Grid>
                            <Image
                                x:Name="LeftMouthWindow"
                                HorizontalAlignment="Center"
                                IsHitTestVisible="True"
                                Source="{Binding LeftCamera.Bitmap}" />
                            <Canvas
                                IsVisible="{Binding LeftCamera.IsCropMode}"
                                x:Name="LeftCanvasWindow"
                                Background="Transparent"
                                IsHitTestVisible="False">
                                <Rectangle
                                    x:Name="LeftRectangleWindow"
                                    Fill="Transparent"
                                    Canvas.Top="{Binding LeftCamera.OverlayRectangle.Top}"
                                    Canvas.Left="{Binding LeftCamera.OverlayRectangle.Left}"
                                    Width="{Binding LeftCamera.OverlayRectangle.Width}"
                                    Height="{Binding LeftCamera.OverlayRectangle.Height}"
                                    IsHitTestVisible="False"
                                    Stroke="Red"
                                    StrokeThickness="2" />
                            </Canvas>
                        </Grid>
                    </Viewbox>

                    <!--  Camera settings section in Expander  -->
                    <Expander
                        Grid.Row="3"
                        HorizontalAlignment="Stretch"
                        Header="Left Camera Settings"
                        IsExpanded="False"
                        IsVisible="{Binding !LeftCamera.IsCropMode}">
                        <Border Background="{DynamicResource SystemChromeMediumLowColor}" CornerRadius="8">
                            <StackPanel>

                                <Grid ColumnDefinitions="Auto,*,Auto" RowDefinitions="Auto,Auto,Auto,Auto">
                                    <StackPanel Grid.Row="0" Grid.Column="0">
                                        <TextBlock
                                            FontSize="14"
                                            FontWeight="Medium"
                                            Text="Vertical Flip" />
                                        <TextBlock FontSize="12" Text="Flip the image vertically" />
                                    </StackPanel>
                                    <CheckBox
                                        Grid.Row="0"
                                        Grid.Column="2"
                                        IsChecked="{Binding LeftCamera.FlipVertically, Mode=TwoWay}" />

                                    <StackPanel
                                        Grid.Row="1"
                                        Grid.Column="0"
                                        Margin="0,12">
                                        <TextBlock
                                            FontSize="14"
                                            FontWeight="Medium"
                                            Text="Horizontal Flip" />
                                        <TextBlock FontSize="12" Text="Flip the image horizontally" />
                                    </StackPanel>
                                    <CheckBox
                                        Grid.Row="1"
                                        Grid.Column="2"
                                        IsChecked="{Binding LeftCamera.FlipHorizontally, Mode=TwoWay}" />

                                    <StackPanel Grid.Row="2" Grid.Column="0">
                                        <TextBlock
                                            FontSize="14"
                                            FontWeight="Medium"
                                            Text="Image Rotation" />
                                        <TextBlock FontSize="12" Text="Rotates the image in a circle" />
                                    </StackPanel>
                                    <Slider
                                        x:Name="LeftRotationSlider"
                                        Grid.Row="3"
                                        Grid.Column="0"
                                        Grid.ColumnSpan="3"
                                        Maximum="6.28"
                                        Minimum="0"
                                        Value="{Binding LeftCamera.Rotation}" />
                                </Grid>
                            </StackPanel>
                        </Border>
                    </Expander>
                </Grid>

